{
	"files.eol": "\n",
	"[typescript]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint",
		"editor.formatOnSave": true
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "dbaeumer.vscode-eslint",
		"editor.formatOnSave": true
	},
	"eslint.run": "onType",
	"eslint.format.enable": true,
	"typescript.tsdk": "node_modules/typescript/lib",
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"include": true,
		".vscode": true,
		"node_modules": true,
		".eslintrc.json": true,
		"**/.eslintrc.json": true,
		".gitignore": true,
		".prettierrc": true,
		"flamework.build": true,
		"README.md": true,
		"tsconfig.json": true,
		"package-lock.json": true
	},
	"explorerExclude.backup": {}
}